#ifndef DATALOADER_H_
#define DATALOADER_H_

namespace redog {

class DataLoader {
public:
	DataLoader();
	//载入数据
	virtual bool load(char *ph);
	//从已经压缩的二进制文件中载入数据
	virtual bool fromfile(char *ph);
	virtual bool tofile(char *ph);
	virtual ~DataLoader();
};

} /* namespace redog */
#endif /* DATALOADER_H_ */
